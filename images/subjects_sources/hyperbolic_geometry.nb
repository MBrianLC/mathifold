Cell[BoxData[
 RowBox[{
  RowBox[{"hyp", "[", 
   RowBox[{"n_", ",", "c_"}], "]"}], ":=", "\[IndentingNewLine]", 
  RowBox[{"(", 
   RowBox[{
    RowBox[{"d", "=", 
     RowBox[{"N", "[", 
      SqrtBox[
       RowBox[{
        RowBox[{"-", "1"}], "+", 
        FractionBox["2", 
         RowBox[{"1", "+", 
          RowBox[{
           RowBox[{"Sec", "[", 
            FractionBox["\[Pi]", "c"], "]"}], " ", 
           RowBox[{"Sin", "[", 
            FractionBox["\[Pi]", "n"], "]"}]}]}]]}]], "]"}]}], ";", 
    "\[IndentingNewLine]", 
    RowBox[{
     RowBox[{"transf", "[", "z_", "]"}], ":=", 
     FractionBox[
      RowBox[{
       RowBox[{
        RowBox[{"(", 
         RowBox[{
          RowBox[{"Exp", "[", 
           FractionBox[
            RowBox[{"2", "\[Pi]", " ", "\[ImaginaryI]"}], "n"], "]"}], "-", 
          SuperscriptBox["d", "2"]}], ")"}], "z"}], "+", 
       RowBox[{"d", 
        RowBox[{"(", 
         RowBox[{"1", "-", 
          RowBox[{"Exp", "[", 
           FractionBox[
            RowBox[{"2", "\[Pi]", " ", "\[ImaginaryI]"}], "n"], "]"}]}], 
         ")"}]}]}], 
      RowBox[{
       RowBox[{"d", 
        RowBox[{"(", 
         RowBox[{
          RowBox[{"Exp", "[", 
           FractionBox[
            RowBox[{"2", "\[Pi]", " ", "\[ImaginaryI]"}], "n"], "]"}], "-", 
          "1"}], ")"}], " ", "z"}], "+", "1", "-", 
       RowBox[{
        SuperscriptBox["d", "2"], 
        RowBox[{"Exp", "[", 
         FractionBox[
          RowBox[{"2", "\[Pi]", " ", "\[ImaginaryI]"}], "n"], "]"}]}]}]]}], 
    ";", "\[IndentingNewLine]", 
    RowBox[{
     RowBox[{"transf2", "[", "z_", "]"}], ":=", 
     RowBox[{"If", "[", 
      RowBox[{
       RowBox[{
        RowBox[{"Re", "[", "z", "]"}], "<", "0"}], ",", 
       RowBox[{"transf2", "[", 
        RowBox[{"-", "z"}], "]"}], ",", 
       RowBox[{"If", "[", 
        RowBox[{
         RowBox[{
          RowBox[{"Im", "[", "z", "]"}], "<", "0"}], ",", 
         RowBox[{"transf2", "[", 
          RowBox[{"Conjugate", "[", "z", "]"}], "]"}], ",", 
         RowBox[{"(", 
          RowBox[{
           RowBox[{"z2", "=", 
            RowBox[{"transf", "[", "z", "]"}]}], ";", 
           RowBox[{"If", "[", 
            RowBox[{
             RowBox[{
              RowBox[{"Abs", "[", "z2", "]"}], "\[GreaterEqual]", 
              RowBox[{"Abs", "[", "z", "]"}]}], ",", "z", ",", 
             RowBox[{"transf2", "[", "z2", "]"}]}], "]"}]}], ")"}]}], "]"}]}],
       "]"}]}], ";", "\[IndentingNewLine]", 
    RowBox[{
     RowBox[{"color", "[", "z_", "]"}], ":=", 
     RowBox[{"(", 
      RowBox[{
       RowBox[{"zz", "=", 
        FractionBox[
         RowBox[{"z", "-", "d"}], 
         RowBox[{"1", "-", 
          RowBox[{"d", " ", "z"}]}]]}], ";", 
       RowBox[{"If", "[", 
        RowBox[{
         RowBox[{
          RowBox[{"Abs", "[", "zz", "]"}], "\[GreaterEqual]", "1"}], ",", 
         RowBox[{"{", 
          RowBox[{"1", ",", "1", ",", "1"}], "}"}], ",", 
         RowBox[{"(", 
          RowBox[{
           RowBox[{"a", "=", 
            RowBox[{"transf2", "[", "zz", "]"}]}], ";", 
           RowBox[{"If", "[", 
            RowBox[{
             RowBox[{
              RowBox[{"Abs", "[", 
               RowBox[{"Re", "[", "a", "]"}], "]"}], "<", "0.04"}], ",", 
             RowBox[{"{", 
              RowBox[{"1", ",", "1", ",", "1"}], "}"}], ",", 
             RowBox[{"{", 
              RowBox[{"0", ",", "0.5", ",", "0.5"}], "}"}]}], "]"}]}], 
          ")"}]}], "]"}]}], ")"}]}], ";", "\[IndentingNewLine]", 
    RowBox[{"Graphics", "[", 
     RowBox[{"Raster", "[", 
      RowBox[{"Table", "[", 
       RowBox[{
        RowBox[{"color", "[", 
         RowBox[{"x", "+", 
          RowBox[{"\[ImaginaryI]", " ", "y"}]}], "]"}], ",", 
        RowBox[{"{", 
         RowBox[{"y", ",", 
          RowBox[{"-", "1"}], ",", "1", ",", "0.002"}], "}"}], ",", 
        RowBox[{"{", 
         RowBox[{"x", ",", 
          RowBox[{"-", "1"}], ",", "1", ",", "0.002"}], "}"}]}], "]"}], "]"}],
      "]"}]}], ")"}]}]], "Input",
